{
	"name": "@fils/ui",
	"version": "0.0.25",
	"description": "Fil Studio UI library written in TypeScript",
	"main": "lib/main.js",
	"repository": "git@github.com:fil-studio/fils.git",
	"homepage": "https://github.com/fil-studio/fils/tree/main/packages/ui",
	"author": "fil studio <hello@fil.studio>",
	"license": "Apache-2.0",
	"scripts": {
		"prepare": "npm run build",
		"build": "node build.js && tsc --emitDeclarationOnly",
		"dev": "chokidar 'src/scss/**/*.scss' -c 'sass src/scss/main.scss:src/bundle/bundle.css && cleancss -o src/bundle/bundle.min.css src/bundle/bundle.css && echo \"scss compiled\"'"
	},
	"files": [
		"lib",
		"README.md",
		"package.json"
	],
	"devDependencies": {
		"chokidar-cli": "^3.0.0",
		"clean-css": "^5.3.2",
		"clean-css-cli": "^5.6.2",
		"esbuild": "^0.17.8",
		"esbuild-plugin-d.ts": "^1.1.0",
		"esbuild-plugin-replace": "^1.3.0",
		"glob": "^8.1.0",
		"sass": "^1.57.1",
		"typescript": "^4.9.5"
	},
	"dependencies": {
		"@fils/color": "^0.0.9",
		"@fils/math": "^0.0.4",
		"@fils/ui-icons": "^0.0.7",
		"@fils/utils": "^0.0.7"
	}
}
